    "esm": "^3.2.25",
    "express": "^4.14.0",
    "express-jwt": "^5.3.0",
    "express-winston": "^2.4.0",
    "js-yaml": "^3.8.3",
    "swagger-jsdoc": "^1.9.7",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.4.6",
    "jest": "^27.4.7",
